bazel_dep(
    name = "rules_swift_tidy",
    version = "0.0.0",
    dev_dependency = True,
)
local_path_override(
    module_name = "rules_swift_tidy",
    path = "../..",
)

bazel_dep(name = "apple_support", version = "1.22.1")
bazel_dep(
    name = "rules_swift",
    version = "2.1.1",
    repo_name = "build_bazel_rules_swift",
)

